public class CornerSpace extends Space {
	int freeParkingMoney;
	String nameOfSpace;
	
	public CornerSpace(String name) {
		nameOfSpace = name;
		freeParkingMoney = 500;
	}
	
	public void goJail() {
		Player newJailer = currentPlayer; // currentPlayer is an instance variable from turn class
		newJailer.changeLocation(10); //setting position on array to 10, which is the integer location of jail
	}
	
	public void addTax() {
		
	}
	
	public void parkingReset() {
		freeParkingMoney=500;
	
	}
	
	public void getParkingMoney() {
		
	}
}
